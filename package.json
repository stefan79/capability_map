  "source": "src/index.html",
  "scripts": {
    "start": "parcel src/index.html --dist-dir dist --no-cache",
    "build": "parcel build ./ --public-url ./ && mkdir -p dist && ([ -d public ] && cp -R public/* dist/ || true) && ([ -d assets ] && cp -R assets dist/ || true) && ([ -d src/assets ] && cp -R src/assets dist/ || true)",

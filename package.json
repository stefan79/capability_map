    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "deploy:forge": "node scripts/deploy-forge.js",
    "generate:aggregated-model": "node scripts/generate-aggregated-model.js"
  },
